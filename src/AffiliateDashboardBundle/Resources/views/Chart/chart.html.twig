{% extends 'base.html.twig' %}

{% block body %}
    <div class="row">
        <div class="col-md-12">
            <div id="chart" style="min-width: 400px; height: 400px; margin: 0 auto"></div>
        </div>
    </div>
{% endblock %}

{% block javascripts %}
    <script src="//code.highcharts.com/4.2.2/highcharts.js"></script>
    <script src="//code.highcharts.com/4.2.2/modules/exporting.js"></script>

    <script type="text/javascript">
        {{ chart(chart) }}
    </script>
{% endblock %}